create database userManagerL12;
use userManagerL12;

create table users
(
    id      int(3)       not null auto_increment,
    name    varchar(120) not null,
    email   varchar(120) not null,
    country varchar(120) not null,
    primary key (id)
);

insert into users(name, email, country)
VALUES ('Batman', 'batman@gmail.com', 'US');
insert into users(name, email, country)
VALUES ('Yugi', 'yugioh@gmail.com', 'JP');
insert into users(name, email, country)
VALUES ('Yersin', 'yersin@gmail.com', 'Fr');